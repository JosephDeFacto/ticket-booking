{% extends 'base.html.twig' %}

{% block body %}

    {{ form_start(form) }}
        {{ form_row(form.title) }}
        {{ form_row(form.description) }}
        {{ form_row(form.duration) }}
        {{ form_row(form.language) }}
        {{ form_row(form.releaseDate) }}
        {{ form_row(form.country) }}
        {{ form_row(form.genre) }}
        {{ form_row(form.image) }}

        {{ form_rest(form) }}

        <button type="submit" name="submit" value="Submit">New!</button>
    {{ form_end(form) }}
{% endblock %}